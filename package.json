{
  "name": "redux-action-transform-middleware",
  "version": "0.1.0",
  "description": "Apply a transformation to a specified target on a Redux action, if that target exists.",
  "main": "dist/index.js",
  "scripts": {
    "build": "mkdir -p dist && babel src/index.js --out-file dist/index.js",
    "dev": "mkdir -p dist && babel src/index.js --watch --out-file dist/index.js"
  },
  "keywords": [
    "recursive",
    "redux",
    "action",
    "actions",
    "transform",
    "middleware"
  ],
  "author": "Ben Wiley",
  "license": "MIT",
  "dependencies": {
    "var-validator": "0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0"
  }
}
